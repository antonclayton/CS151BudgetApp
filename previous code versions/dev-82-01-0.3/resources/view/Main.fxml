<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="mainBox" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="100.0" style="-fx-background-color: darkblue;">
         <children>
            <Button layoutX="15.0" layoutY="25.0" mnemonicParsing="false" onAction="#loadHomePageOp" prefHeight="10.0" prefWidth="70.0" text="Home" />
            <Button layoutX="15.0" layoutY="70.0" mnemonicParsing="false" onAction="#goToAccountPageOp" prefHeight="10.0" prefWidth="70.0" text="Accounts" />
         </children></AnchorPane>
   </children>
</HBox>
